<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="165.0" prefWidth="332.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ProgressBarController">
   <bottom>
      <AnchorPane prefHeight="169.0" prefWidth="332.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="87.0" layoutY="14.0" text="Generating data" textAlignment="CENTER">
               <font>
                  <Font name="Candara Italic" size="24.0" />
               </font></Label>
            <ProgressBar fx:id="generateDataProgressBar" layoutX="44.0" layoutY="55.0" prefHeight="29.0" prefWidth="245.0" progress="0.0" />
            <Label layoutX="46.0" layoutY="92.0" text="Start:" textAlignment="CENTER">
               <font>
                  <Font name="Candara Italic" size="18.0" />
               </font>
            </Label>
            <Label fx:id="initialTimeLabel" layoutX="105.0" layoutY="92.0" text="- - -" textAlignment="CENTER">
               <font>
                  <Font name="Candara Italic" size="18.0" />
               </font>
            </Label>
            <Label layoutX="54.0" layoutY="118.0" text="End:" textAlignment="CENTER">
               <font>
                  <Font name="Candara Italic" size="18.0" />
               </font>
            </Label>
            <Label fx:id="finalTimeLabel" layoutX="105.0" layoutY="118.0" text="- - -" textAlignment="CENTER">
               <font>
                  <Font name="Candara Italic" size="18.0" />
               </font>
            </Label>
            <Button fx:id="continueButton" layoutX="262.0" layoutY="138.0" mnemonicParsing="false" onAction="#continueApp" text="Continue" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
