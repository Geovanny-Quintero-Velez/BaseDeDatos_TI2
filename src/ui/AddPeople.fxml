<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="290.0" prefWidth="291.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <AnchorPane prefHeight="290.0" prefWidth="249.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="createButton" layoutX="198.0" layoutY="249.0" mnemonicParsing="false" onAction="#Create" prefHeight="27.0" prefWidth="73.0" text="Create">
               <font>
                  <Font name="Candara Bold" size="14.0" />
               </font></Button>
            <Label layoutX="98.0" layoutY="14.0" text="Add a Person" textAlignment="CENTER">
               <font>
                  <Font name="Candara Italic" size="19.0" />
               </font></Label>
            <DatePicker fx:id="dateSelector" layoutX="21.0" layoutY="106.0" prefHeight="26.0" prefWidth="139.0" promptText="Birthdate" />
            <TextField fx:id="hourField" alignment="CENTER" layoutX="62.0" layoutY="55.0" prefHeight="30.0" prefWidth="80.0" promptText="Name" />
            <TextField fx:id="minuteField" alignment="CENTER" layoutX="154.0" layoutY="55.0" prefHeight="30.0" prefWidth="80.0" promptText="S. Name" />
            <ComboBox fx:id="genderSelector" layoutX="175.0" layoutY="107.0" prefHeight="25.0" prefWidth="85.0" promptText="Gender" visibleRowCount="5" />
            <ComboBox fx:id="nationatilySelector" layoutX="72.0" layoutY="153.0" prefHeight="25.0" prefWidth="140.0" promptText="Nationality" />
            <TextField fx:id="heigthField" alignment="CENTER" layoutX="146.0" layoutY="197.0" prefHeight="30.0" prefWidth="60.0" promptText="Meters" />
            <Label layoutX="86.0" layoutY="201.0" prefHeight="22.0" prefWidth="57.0" text="Heigth :" textAlignment="CENTER">
               <font>
                  <Font name="Candara Italic" size="17.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
